@startuml
title Planområde for planlegging igangsatt - innsynstjeneste - Objekttyper

skinparam backgroundColor #F5F5F5
skinparam shadowing false
skinparam RoundCorner 6
skinparam ArrowColor #6C8198
skinparam class {
  BackgroundColor #FBF2E8
  BorderColor #9C8578
  FontColor #2D201A
  HeaderBackgroundColor #EFE1D6
  HeaderFontColor #2D201A
  AttributeIconSize 0
}
skinparam note {
  BackgroundColor #FFFFFF
  BorderColor #6C8198
  FontColor #2D201A
}
skinparam stereotypeCBackgroundColor #EAD9CE
skinparam stereotypeCBorderColor #9C8578
skinparam stereotypeCFontColor #2D201A

package "Objekttyper" {

  class Arealplan <<featureType>> {
    + objid [0..1] : Integer
    + nasjonalArealplanId [0..1] : Object
    + plannavn [0..1] : CharacterString
    + plantype [0..1] : CharacterString
    + kunngjøringsdatoVarselOmPlanoppstart [0..1] : Date
    + forslagsstillertype [0..1] : CharacterString
    + lovreferanse [0..1] : CharacterString
    + identifikasjon [0..1] : Object
    + forsteDigitaliseringsdato [0..1] : DateTime
    + oppdateringsdato [0..1] : DateTime
    + link [0..1] : CharacterString
    + linkPlanomrade [0..1] : CharacterString
    + linkPlandokumenter [0..1] : CharacterString

    ..Geometri..
    + geometry [1] : GM_Object
  }

  Arealplan *-- Arealplan_nasjonalArealplanId : nasjonalArealplanId [0..1]
  Arealplan *-- Arealplan_identifikasjon : identifikasjon [0..1]

  class Arealplan_nasjonalArealplanId {
    + nasjonalArealplanId.planid [0..1] : CharacterString
    + nasjonalArealplanId.kommunenummer [0..1] : CharacterString
  }

  class Arealplan_identifikasjon {
    + identifikasjon.lokalId [0..1] : CharacterString
    + identifikasjon.navnerom [0..1] : CharacterString
    + identifikasjon.versjonId [0..1] : CharacterString
  }

  class "Planområde" as Planomr_de <<featureType>> {
    + objid [0..1] : Integer
    + identifikasjon [0..1] : Object
    + arealplan [0..1] : Integer
    + førsteDigitaliseringsdato [0..1] : DateTime
    + nasjonalArealplanId [0..1] : Object
    + plannavn [0..1] : CharacterString
    + plantype [0..1] : CharacterString
    + kunngjøringsdatoVarselOmPlanoppstart [0..1] : Date
    + forslagsstillertype [0..1] : CharacterString
    + lovreferanse [0..1] : CharacterString
    + oppdateringsdato [0..1] : DateTime
    + link [0..1] : CharacterString
    + linkArealplan [0..1] : CharacterString
    + linkPlandokumenter [0..1] : CharacterString

    ..Geometri..
    + geometry [1] : GM_Object
  }

  Planomr_de *-- Planomr_de_identifikasjon : identifikasjon [0..1]
  Planomr_de *-- Planomr_de_nasjonalArealplanId : nasjonalArealplanId [0..1]

  class Planomr_de_identifikasjon {
    + identifikasjon.lokalId [0..1] : CharacterString
    + identifikasjon.navnerom [0..1] : CharacterString
    + identifikasjon.versjonId [0..1] : CharacterString
  }

  class Planomr_de_nasjonalArealplanId {
    + nasjonalArealplanId.planid [0..1] : CharacterString
    + nasjonalArealplanId.kommunenummer [0..1] : CharacterString
  }

  class object {
    + planid [0..1] : CharacterString
    + kommunenummer [0..1] : CharacterString
  }
}

@enduml
